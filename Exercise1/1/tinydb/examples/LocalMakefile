	$(BUILDEXE)
	
bin/ex2$(EXEEXT): bin/examples/Exercise1.2.o $(obj)
	$(BUILDEXE)


# Alias for all examples
examples_bin: bin/scanexample$(EXEEXT) bin/selectexample$(EXEEXT) bin/joinexample$(EXEEXT) bin/chiexample$(EXEEXT) bin/ex1$(EXEET) bin/ex2$(EXEET)
